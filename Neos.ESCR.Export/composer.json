{
    "description": "Import/Export functionality for Neos supporting the Event Sourced Content Repository",
    "type": "neos-package",
    "name": "neos/escr-export",
    "require": {
        "php": ">=8.1",
        "league/flysystem": "^3",
        "neos/neos": "*",
        "neos/event-sourcing": "*",
        "neos/content-repository": "*",
        "neos/escr-asset-usage": "*",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan": "^1.5"
    },
    "suggest": {
        "league/flysystem-ziparchive": "to export zip archives"
    },
    "autoload": {
        "psr-4": {
            "Neos\\ESCR\\Export\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Neos.ESCR.Export"
        }
    }
}
